{
 "languageserver": {
        "intelephense": {
            "command": "intelephense",
            "args": ["--stdio"],
            "filetypes": ["php"],
            "initializationOptions": {
                "storagePath": "/tmp/intelephense"
            }
        },
        "psalm": {
            "command": "psalm-language-server",
            "args": ["--find-dead-code"],
            "filetypes": ["php"]
        }
    },
"coc.preferences.colorSupport": true
}
